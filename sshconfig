IdentityFile /home/brian/.ssh/old_keys/brian_rsa

Host *
    ControlMaster auto
    ControlPath ~/.ssh/ssh-%r@%h:%p
    ControlPersist 4h

host ro.lan
    hostname ro.lan
    port 2220
    IdentityFile /home/brian/.ssh/brian_rsa

host router 
    hostname router.lan
    port 2220
    user root
    IdentityFile /home/brian/.ssh/brian_rsa

host github.com
    hostname github.com
    user iPenguin
    IdentityFile /home/brian/.ssh/github_rsa

host bcmilco.homelinux.org
    hostname bcmilco.homelinux.org
    user brian
    IdentityFile /home/brian/.ssh/brian_rsa

Host server.lan
    HostName server.lan
    User brian
    IdentityFile /home/brian/.ssh/brian_rsa

Host server
    HostName server
    User brian
    IdentityFile /home/brian/.ssh/brian_rsa
